#include <stdio.h>
 int main()
 {
    int x = 10

    printf("x = %d\n",x);
    printf("x = %d\n",x++);  // print x first, then increments the value in the under line.
    printf("x = %d\n",x);
    printf("\n\n");
   
    int a = 10;
    
    printf("a = %d\n",a);
    printf("a = %d\n",++a); 
    printf("a = %d\n",a);
    return;0

    
}
