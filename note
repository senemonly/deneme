#include<stdio.h>
int main() {
    int x=9;
    float y=1.5;

    printf("%f\n",x/y);
    printf("%d\n",(int)0.5);
    printf("%d\n",(int)2.5);
    printf("%f\n",2.1*3);
    printf("%d\n",(int)3.1*6);//int değerine çevirmek istediğimiz değerin önüne paranetz içinde (int) ekliyoruz.
    printf("%f\n",(float)3);
    printf("%d\n",3);
    printf("%f\n",(float)3);
    return 0;


}
